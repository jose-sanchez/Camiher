<Window x:Class="AdministrationCenter.BuyProduct"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="BuyProduct" Height="184" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="112*" />
        </Grid.RowDefinitions>
        <Border Grid.Column="0" Grid.Row="0">
        <Label Content="Precio Cliente"></Label>
        </Border>
        <Border Grid.Column="0" Grid.Row="1">
            <Label Content="Precio Final"></Label>
        </Border>
        <Border Grid.Column="1" Grid.Row="0">
            <TextBox Name="tbPrecioCliente"></TextBox>
        </Border>
        <Border Grid.Column="1" Grid.Row="1">
            <TextBox Name="tbPrecioFinal"></TextBox>
        </Border>
        <Button Content="Comprar" Grid.ColumnSpan="2" Grid.Row="4" Height="23" HorizontalAlignment="Left" Margin="100,47,0,0" Name="btBuy" VerticalAlignment="Top" Width="75" Click="btBuy_Click" />
    </Grid>
</Window>
